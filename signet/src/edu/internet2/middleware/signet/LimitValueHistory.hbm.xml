<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 2.0//EN"
          "http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd">
<hibernate-mapping>
	<class
		name="edu.internet2.middleware.signet.LimitValueHistory"
		table="signet_assignmentLimit_history"
		lazy="true">
		
		<id name="historyId" type="int" column="historyID">
			<generator class="native"/>
		</id>

		<property
			name="assignmentId"
			column="assignmentID"
			not-null="true" />
		<property
			name="instanceNumber"
			not-null="true" />
			
		<many-to-one
			name="limit"
			column="limitKey"
			not-null="true"
			class="edu.internet2.middleware.signet.LimitImpl"/>
		
		<property
			name="value">
		</property> 

	</class>
</hibernate-mapping>
