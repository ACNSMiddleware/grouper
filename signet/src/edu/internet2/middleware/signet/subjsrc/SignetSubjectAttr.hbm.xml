<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 2.0//EN"
          "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<!-- $Header: /home/hagleyj/i2mi/signet/src/edu/internet2/middleware/signet/subjsrc/SignetSubjectAttr.hbm.xml,v 1.6 2007-03-05 18:10:51 ddonn Exp $ -->

<hibernate-mapping
		default-lazy="false" >

	<class
			name="edu.internet2.middleware.signet.subjsrc.SignetSubjectAttr"
			table="signet_subjectattribute" >
		<id name="subjectAttr_PK"
				type="long"
				column="subjectAttributeKey"
				unsaved-value="null" >
			<generator class="sequence">
				<param name="sequence">subjectAttributeSerial</param>
			</generator>
		</id>
		<timestamp name="modifyDate" column="modifyDatetime" />

		<property name="mappedName"	column="attributeName"		type="string"	not-null="true" />
		<property name="attrValue" column="attributeValue"		type="string"	not-null="true" />
		<property name="attrType"	column="attributeType"		type="string"	not-null="true" />
		<property name="sequence"	column="attributeSequence"	type="integer"	not-null="true" />
		<many-to-one name="parent"
				class="edu.internet2.middleware.signet.subjsrc.SignetSubject"
				column="subjectKey"
				cascade="save-update" >
		</many-to-one>
	</class>
</hibernate-mapping>
