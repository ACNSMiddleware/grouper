<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 2.0//EN"
          "http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd">
<!-- $Header: /home/hagleyj/i2mi/signet/src/edu/internet2/middleware/signet/subjsrc/SignetSubjectAttr.hbm.xml,v 1.2 2006-11-30 04:21:49 ddonn Exp $ -->

<hibernate-mapping>
	<class
		name="edu.internet2.middleware.signet.subjsrc.SignetSubjectAttr"
		table="signet_subjectattribute"
		lazy="false" >

		<id name="subjectAttr_PK"
				type="long"
				column="subjectAttrKey"
				unsaved-value="null" >
			<generator class="native" />
		</id>

		<property name="mappedName"	column="name"			type="string" />
        <property name="modifyDate"	column="modifyDatetime"	type="date" />

		<list name="sourceValues"
				cascade="all"
				lazy="false"
				inverse="true" >
			<key column="subjectAttrKey" />
			<index column="sequence" type="long" />
			<one-to-many class="edu.internet2.middleware.signet.subjsrc.SignetSubjectAttrValue" />
		</list>

		<many-to-one name="parent"
				class="edu.internet2.middleware.signet.subjsrc.SignetSubject"
				column="subjectKey"
				not-null="true" >
		</many-to-one>

	</class>
</hibernate-mapping>
