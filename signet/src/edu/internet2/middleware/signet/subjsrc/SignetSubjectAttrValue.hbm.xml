<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 2.0//EN"
          "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<!-- $Header: /home/hagleyj/i2mi/signet/src/edu/internet2/middleware/signet/subjsrc/SignetSubjectAttrValue.hbm.xml,v 1.2 2007-01-16 18:21:21 ddonn Exp $ -->

<hibernate-mapping>
	<class
		name="edu.internet2.middleware.signet.subjsrc.SignetSubjectAttrValue"
		table="signet_subjectattrvalue"
		lazy="false" >
		
		<id name="subjectAttrValue_PK"
				column="subjectattrvaluekey"
				type="long"
				unsaved-value="null" >
			<generator class="native" />
		</id>

		<property name="value"
				column="value"
				type="string"
				not-null="true"
		/>

		<property name="type"
				column="type"
				type="string"
				not-null="true"
		/>

		<property name="sequence"
				column="sequence"
				type="long"
				not-null="true" > 
		</property>

		<many-to-one name="parent"
			class="edu.internet2.middleware.signet.subjsrc.SignetSubjectAttr"
			column="subjectAttrKey"
			not-null="true"
		/>

	</class>
</hibernate-mapping>
