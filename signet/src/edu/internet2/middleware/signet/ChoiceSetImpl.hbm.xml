<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 2.0//EN"
          "http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd">
<hibernate-mapping>
	<class
		name="edu.internet2.middleware.signet.ChoiceSetImpl"
		table="ChoiceSet">
		
		<id name="id" type="string" column="choiceSetID">
			<generator class="assigned"/>
		</id>
		<timestamp name="modifyDatetime"/>
	
		<property
			name="adapterClassName"
			column="adapterClass"
			not-null="true">
		</property>	
		
		<many-to-one 
			name="subsystem"
			column="subsystemID"
			class="edu.internet2.middleware.signet.SubsystemImpl"
			not-null="true"/>
		
		<set name="choices" inverse="true" cascade="save-update">
			<key column="choiceSetID"/>
			<one-to-many
				class="edu.internet2.middleware.signet.ChoiceImpl"/>
		</set>
		
	</class>
</hibernate-mapping>
