<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 2.0//EN"
          "http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd">
<hibernate-mapping>
	<class
		name="edu.internet2.middleware.signet.ChoiceImpl"
		table="signet_choice">
		
		<composite-id
			name="fullyQualifiedId"
			class="edu.internet2.middleware.signet.ChoiceFullyQualifiedId">
			<key-property name="choiceSetId" column="choiceSetID" />
			<key-property name="choiceValue" column="value" />
		</composite-id>
		
		<timestamp name="modifyDatetime"/>
	
		<property
			name="displayValue" column="label" type="string" not-null="true"/>
		<property
			name="rank" type="integer" not-null="true"/>
		<property
			name="displayOrder" type="integer" not-null="true"/>
	</class>
</hibernate-mapping>
