<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 2.0//EN"
          "http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd">
<hibernate-mapping>
	<class
		name="edu.internet2.middleware.signet.AssignmentLimitValue"
		table="AssignmentLimitValue">
		
		<composite-id>
			
			<key-property
				name="assignmentId"
				type="int">
				<column name="assignmentID"></column>
			</key-property>
			
			<key-property
				name="limitId"
				type="string">
				<column name="limitID"></column>
			</key-property>

			<key-property
				name="value"
				type="string">
				<column name="value"></column>
			</key-property> 

		</composite-id>
	</class>
</hibernate-mapping>
