<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping
    PUBLIC "-//Hibernate/Hibernate Mapping DTD//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd">

<!-- $Id: Stem.hbm.xml,v 1.1.2.5 2005-10-17 17:43:10 blair Exp $ -->

<hibernate-mapping package="edu.internet2.middleware.grouper">
  <class name="Stem" table="grouper_stems">

    <meta attribute="class-description">
A namespace within the Groups Registry.
@author blair christensen.
    </meta>
    <meta attribute="implement-equals">true</meta>
    <meta attribute="session-method">HibernateUtil.getSession();</meta>

    <!-- id -->
    <id name="id" type="string">
      <meta attribute="use-in-tostring">false</meta>
      <column name="id" sql-type="char(32)"/>
      <generator class="uuid.hex"/>
    </id>
    <!-- id -->
    <!-- version -->
    <version name="version"/>
    <!-- version -->


    <!-- child_groups -->
    <set name="child_groups">
      <meta attribute="field-description">
Get child groups.
      </meta>
      <meta attribute="scope-get">public</meta>
      <key column="id"/>
      <one-to-many class="edu.internet2.middleware.grouper.Group"/>
    </set>
    <!-- child_groups -->

    <!-- child_stems -->
    <set name="child_stems">
      <meta attribute="field-description">
Get child stems.
      </meta>
      <meta attribute="scope-get">public</meta>
      <key column="id"/>
      <one-to-many class="edu.internet2.middleware.grouper.Stem"/>
    </set>
    <!-- child_stems -->

    <!-- creator_id -->
    <many-to-one
      name="creator_id"
      class="edu.internet2.middleware.grouper.Member" 
    >
      <meta attribute="field-description">
Get creator.
      </meta>
      <meta attribute="finder-method">findByCreator</meta>
      <meta attribute="use-in-equals">true</meta>
      <meta attribute="use-in-tostring">true</meta>
      <column name="creator_id" sql-type="char(32)"/>
    </many-to-one>
    <!-- creator_id -->

    <!-- create_source -->
    <property 
      name="create_source"
      type="string"
      length="255"
    >
      <meta attribute="field-description">
Get create source.
      </meta>
    </property>
    <!-- create_source -->

    <!-- create_time -->
    <property 
      name="create_time"
      type="java.util.Date"
      not-null="true"
      update="false"
    >
      <meta attribute="field-description">
Get create time.
      </meta>
      <meta attribute="scope-get">public</meta>
    </property>
    <!-- create_time -->

    <!-- description -->
    <property 
      name="description"
      type="string"
      length="1024"
    >
      <meta attribute="field-description">
Get description.
      </meta>
      <meta attribute="scope-get">public</meta>
    </property>
    <!-- description -->

    <!-- display_extension -->
    <property
      name="display_extension"
      type="string"
      length="255"
    >
      <meta attribute="field-description">
Get displayExtension.
      </meta>
      <meta attribute="scope-get">public</meta>
    </property>
    <!-- display_extension -->

    <!-- display_name -->
    <property
      name="display_name"
      type="string"
      length="255"
    >
      <meta attribute="field-description">
Get displayName.
      </meta>
      <meta attribute="scope-get">public</meta>
      <meta attribute="use-in-tostring">true</meta>
    </property>
    <!-- display_name -->

    <!-- extension -->
    <property
      name="extension"
      type="string"
      length="255"
    >
      <meta attribute="field-description">
Get extension.
      </meta>
      <meta attribute="scope-get">public</meta>
    </property>
    <!-- extension -->

    <!-- modifier_id -->
    <many-to-one
      name="modifier_id"
      class="edu.internet2.middleware.grouper.Member" 
    >
      <meta attribute="field-description">
Get modifier.
      </meta>
      <meta attribute="finder-method">findByModifier</meta>
      <meta attribute="use-in-equals">true</meta>
      <meta attribute="use-in-tostring">true</meta>
      <column name="modifier_id" sql-type="char(32)"/>
    </many-to-one>
    <!-- modifier_id -->

    <!-- modify_source -->
    <property 
      name="modify_source"
      type="string"
      length="255"
    >
      <meta attribute="field-description">
Get modify source.
      </meta>
    </property>
    <!-- modify_source -->

    <!-- modify_time -->
    <!-- modify_time -->
    <property 
      name="modify_time"
      type="java.util.Date"
    >
      <meta attribute="class-description">
Get last modify time.
      </meta>
      <meta attribute="scope-get">public</meta>
    </property>
    <!-- modify_time -->

    <!-- name -->
    <property
      name="name"
      type="string"
      length="255"
    >
      <meta attribute="field-description">
Get name.
      </meta>
      <meta attribute="scope-get">public</meta>
      <meta attribute="use-in-tostring">true</meta>
    </property>
    <!-- name -->

    <!-- parent_stem -->
    <many-to-one
      name="parent_stem"
      class="edu.internet2.middleware.grouper.Stem"
    >
      <meta attribute="field-description">
Get parent stem.
      </meta>
      <meta attribute="scope-get">public</meta>
      <column name="parent_stem" sql-type="char(32)"/>
    </many-to-one>
    <!-- parent_stem -->

    <!-- uuid -->
    <property 
      name="uuid"
      type="string"
      length="32"
      not-null="true"
    >
      <meta attribute="field-description">
Get UUID.
      </meta>
      <meta attribute="finder-method">findByUuid</meta>
      <meta attribute="scope-get">public</meta>
      <meta attribute="use-in-equals">true</meta>
      <meta attribute="use-in-tostring">true</meta>
    </property>
    <!-- uuid -->

  </class>
</hibernate-mapping>

