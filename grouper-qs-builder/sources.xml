  <source adapterClass="edu.internet2.middleware.grouper.GrouperSourceAdapter">
    <id>grouperAdapter</id>
    <name>Grouper Adapter</name>
    <type>group</type>
  </source>
    <source adapterClass="edu.internet2.middleware.subject.provider.JDBCSourceAdapter">
    <id>personQS</id>
    <name>UOB QuickStart</name>
    <type>person</type>
    <init-param>
      <param-name>maxActive</param-name>
      <param-value>50</param-value>
    </init-param>
    <init-param>
      <param-name>readOnly</param-name>
      <param-value>true</param-value>
    </init-param>
    <init-param>
      <param-name>maxIdle</param-name>
      <param-value>5</param-value>
    </init-param>
    <init-param>
      <param-name>maxWait</param-name>
      <param-value>5</param-value>
    </init-param>
    <init-param>
      <param-name>dbDriver</param-name>
      <param-value>org.hsqldb.jdbcDriver</param-value>
    </init-param>
    <init-param>
      <param-name>dbUrl</param-name>
      <param-value>jdbc:hsqldb:hsql://localhost:9001</param-value>
    </init-param>
    <init-param>
      <param-name>dbUser</param-name>
      <param-value>sa</param-value>
    </init-param>
  </source>
